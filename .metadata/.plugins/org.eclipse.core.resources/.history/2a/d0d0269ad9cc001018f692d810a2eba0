package com.inheritance;

public class Y extends X{
	int y;
	
	Y(int a,int x,int y){
		super(a);
		this.x=x;
		this.y=y;
		
	}
	
	public static void main(String[] args) {
		Y y1 = new Y();
		System.out.println(y1.x);
		System.out.println(y1.y);
		
		
		X x2 = new X();
		System.out.println(x2.x);
	}
	
	

}
