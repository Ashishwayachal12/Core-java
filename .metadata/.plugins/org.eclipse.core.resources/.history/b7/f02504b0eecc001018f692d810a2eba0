package com.nonprimitivetypecasting;

public class SbiTest {
	public static void main(String[] args) {
		Sbi s = new Sbi("state bank of india", "SBI99588", "hebala");
		System.out.println(s.bName);
		System.out.println(s.ifsc);
		System.out.println(s.branch);

		// upcasting
		Bank b = s;
		System.out.println(b.bName);
		System.out.println(b.ifsc);

	}

}
